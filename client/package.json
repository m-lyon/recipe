{
    "name": "recipe",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "experimental": "vite build --outDir=dist/experimental --base=/recipe-experimental --mode=experimental",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "test": "NODE_ENV=test vitest --workspace=vitest.default.config.ts",
        "test:browser": "NODE_ENV=test vitest --workspace=vitest.browser.config.ts",
        "test-ui": "npm run test -- --ui",
        "test-coverage": "npm run test -- run --coverage",
        "generate": "npm run rm-generated && NODE_ENV=development graphql-codegen",
        "generate:test": "npm run rm-generated && NODE_ENV=test graphql-codegen",
        "rm-generated": "[ -d src/__generated__/ ] && rm -r src/__generated__/ || true",
        "vitest-preview": "vitest-preview",
        "check-types": "tsc --noEmit"
    },
    "dependencies": {
        "@apollo/client": "^3.8.1",
        "@chakra-ui/react": "^3.28.0",
        "@emotion/react": "^11.14.0",
        "@types/apollo-upload-client": "^18.0.0",
        "@types/react-dom": "^18.2.5",
        "apollo-upload-client": "^18.0.1",
        "fraction-unicode": "^1.0.1",
        "framer-motion": "^12.23.24",
        "graphql": "^16.8.0",
        "immer": "^10.0.2",
        "match-sorter": "^6.3.1",
        "mathjs": "^12.2.0",
        "next-themes": "^0.4.6",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-icons": "^5.5.0",
        "react-infinite-scroll-component": "^6.1.0",
        "react-responsive-masonry": "^2.1.7",
        "react-router-dom": "^6.18.0",
        "react-simple-star-rating": "^5.1.7",
        "react-textarea-autosize": "^8.5.3",
        "react-use": "^17.5.0",
        "use-debounce": "^10.0.3",
        "use-deep-compare-effect": "^1.8.1",
        "yup": "^1.3.2",
        "zustand": "^5.0.2"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^5.0.5",
        "@graphql-codegen/client-preset": "^4.7.0",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^14.0.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/dotenv-flow": "^3.3.3",
        "@types/node": "^20.12.3",
        "@types/react": "^18.2.15",
        "@types/react-dom": "^18.2.7",
        "@types/react-responsive-masonry": "^2.1.3",
        "@typescript-eslint/eslint-plugin": "^7.3.1",
        "@typescript-eslint/parser": "^7.3.1",
        "@vitest/browser": "^3.1.1",
        "@vitest/ui": "^3.1.1",
        "camelcase": "^6.3.0",
        "dotenv-flow": "^3.3.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-import-line-sorter": "^1.0.4",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-react": "^7.34.1",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-react-refresh": "^0.4.3",
        "happy-dom": "^15.7.4",
        "playwright": "^1.49.1",
        "typescript": "^5.0.2",
        "vite": "^6.2.6",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.1.1",
        "vitest-browser-react": "^0.1.1",
        "vitest-fetch-mock": "^0.4.5"
    }
}
